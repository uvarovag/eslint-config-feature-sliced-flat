{
    "name": "@uvarovag/eslint-config-feature-sliced-flat",
    "version": "1.0.10",
    "description": "@feature-sliced/eslint-config adapter for ESLint 9 Flat Config",
    "main": "eslint.config.js",
    "type": "module",
    "scripts": {
        "fix": "prettier --write '**/*.{ts,tsx,js,json,css,html,md}' && npm pkg fix",
        "deploy": "npm run fix && npm publish"
    },
    "keywords": [
        "eslint",
        "eslintconfig",
        "eslint-config",
        "feature-sliced",
        "feature-slices",
        "feature-driven",
        "feature-based"
    ],
    "author": "@uvarovag",
    "license": "MIT",
    "homepage": "https://github.com/uvarovag/eslint-config-feature-sliced-flat/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/uvarovag/eslint-config-feature-sliced-flat.git"
    },
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@feature-sliced/eslint-config": "^0.1.1",
        "eslint-plugin-boundaries": "^5.0.1",
        "eslint-plugin-import": "^2.31.0"
    },
    "devDependencies": {
        "@uvarovag/prettier-config": "^1.0.6",
        "prettier": "^3.4.1"
    },
    "peerDependencies": {
        "eslint": "^9.20.1"
    }
}
